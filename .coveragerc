[report]
omit =
    */python?.?/*
    */site-packages/nose/*
    /usr/share/pyshared/*
    */lib-python/?.?/*.py
    */lib_pypy/_*.py
    */site-packages/ordereddict.py
    */site-packages/nose/*
    */unittest2/*
    fatiando/gui/*
    fatiando/_version.py

exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError

[run]
omit =
    */python?.?/*
    */site-packages/nose/*
    /usr/share/pyshared/*
    */lib-python/?.?/*.py
    */lib_pypy/_*.py
    */site-packages/ordereddict.py
    */site-packages/nose/*
    */unittest2/*
    fatiando/gui/*
    fatiando/_version.py
